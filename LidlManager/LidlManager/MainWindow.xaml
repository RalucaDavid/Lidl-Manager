<Window x:Class="LidlManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LidlManager.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="NoResize" Height="800" Width="1400">
    <Window.DataContext>
        <local:MenuCommands/>
    </Window.DataContext>
    <Grid>
        <Frame Name="MainFrame" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
